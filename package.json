    "Arhun Hınçalan <a.hincalan@panates.com> (http://www.npmjs.com/~ahincalan)"
  ],
  "browser": "./lib/browser.js",
  "main": "./lib/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gqlorg/gql-fetch.git"
  },
  "keywords": [
    "javascript",
    "graphql",
    "fetch",
    "client"
  ],
  "dependencies": {
    "node-fetch": "^2.3.0",
    "whatwg-fetch": ">=0.10.0",
    "abortcontroller-polyfill": "^1.2.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/express": "^4.16.1",
    "@types/express-graphql": "^0.6.2",
    "@types/graphql": "^14.0.5",
    "@types/mocha": "^5.2.5",
    "@types/node-fetch": "^2.1.6",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.1.1",
    "mocha": "^5.2.0",
    "nyc": "^13.2.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">= 6.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib/"
  ],
  "nyc": {
    "temp-dir": "./coverage/.nyc_output"
  },
  "scripts": {
    "test": "mocha --require ./test/support/env --reporter spec --bail --check-leaks test/",
    "cover": "nyc --reporter html --reporter text npm run test",
    "travis-test": "mocha --require ./test/support/env-travis --require ./test/support/env --reporter spec --check-leaks test/",
    "travis-cover": "nyc --reporter lcovonly npm run travis-test",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  }
