{
  "name": "gql-fetch",
  "description": "The GraphQL client powered by fetch. Supports multiform and upload files",
  "version": "0.3.8",
  "author": "Panates",
  "contributors": [
    "Eray Hanoglu <e.hanoglu@panates.com>",
    "Arhun Hınçalan <ahincalan@hotmail.com>"
  ],
  "browser": "./lib/browser.js",
  "main": "./lib/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gqlorg/gql-fetch.git"
  },
  "keywords": [
    "javascript",
    "graphql",
    "fetch",
    "client"
  ],
  "dependencies": {
    "abortcontroller-polyfill": "^1.3.0",
    "form-data": "^2.3.3",
    "http-errors": "^1.7.2",
    "node-fetch": "^2.3.0",
    "rxjs": "^6.4.0",
    "whatwg-fetch": ">=0.10.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/express-graphql": "^0.6.2",
    "@types/form-data": "^2.2.1",
    "@types/http-errors": "^1.6.1",
    "@types/graphql": "^14.2.0",
    "@types/mocha": "^5.2.5",
    "@types/node-fetch": "^2.3.2",
    "express": "^4.16.4",
    "express-graphql": "^0.8.0",
    "gql-multipart": "^0.1.1",
    "graphql": "^14.2.1",
    "mocha": "^6.1.4",
    "nyc": "^14.0.0",
    "rejected-or-not": "^1.0.1",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.4"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">= 6.0"
  },
  "files": [
    "LICENSE",
    "README.md",
    "lib/"
  ],
  "nyc": {
    "temp-dir": "./coverage/.nyc_output",
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts",
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.d.ts"
    ]
  },
  "scripts": {
    "test": "mocha -r ./test/support/env -r ts-node/register --reporter spec --bail --check-leaks ./test/*.ts --timeout 20000",
    "cover": "nyc --reporter html --reporter text -r lcov npm run test",
    "travis-cover": "nyc --reporter lcovonly -r lcov npm run test",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "tsc"
  }
}
